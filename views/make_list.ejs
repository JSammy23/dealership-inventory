<h2><%= title %></h2>
<div class="row">
    <div class="col">
        <% if(make_list && make_list.length > 0) { %>
            <ul>
                <% make_list.forEach(make => { %>
                    <li>
                        <%= make.name %> <!-- Assuming each make object has a name property -->
                    </li>
                <% }) %>
            </ul>
        <% } else { %>
            <p>No Makes available.</p>
        <% } %>
    </div>
</div>